" Map Colemak keys to QWERTY keys (in alphabetical order).
noremap e k
noremap f e
noremap g t
noremap i l
noremap j y
noremap k n
noremap l u
noremap n j

noremap r s
noremap s d
noremap t f
noremap u i

noremap E K
noremap F E
noremap G T
noremap I L
noremap J Y
noremap K N
noremap L U
noremap N J

noremap R S
noremap S D
noremap T F
noremap U I

" Set Tab to 2 spaces
set sts=2
